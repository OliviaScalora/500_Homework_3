dplyr::select(-x1,-y1)%>%
mutate(Variable='AGGRESSIVE')
Cross_1617<-CrossTable(mydata$DRIVER1617, mydata$DRINKING_D, prop.r=FALSE, prop.t=FALSE, prop.chisq=FALSE, chisq=TRUE)
