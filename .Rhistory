dplyr::select(-x1,-y1)%>%
mutate(Variable='DRIVER1617')
Cross_65P<-CrossTable(mydata$DRIVER65PLUS, mydata$DRINKING_D, prop.r=FALSE, prop.t=FALSE, prop.chisq=FALSE, chisq=TRUE)
Cross_65P_tbl<-
cbind(as.data.frame(Cross_65P$t)%>%filter(x==1)%>%rename(N=Freq),
